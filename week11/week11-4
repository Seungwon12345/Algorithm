def solution(s):
    count_o = 0 # (
    count_c = 0 # )
    
    for i in range(len(s)):
        if s[i] == '(':
            count_o += 1
        elif s[i] == ')':
            count_c += 1
            
        if count_c > count_o:
            return False  # 처음에 ) 오거나 중간에 ) 와서 짝이 안맞으면 False  ")()("
    
    if count_o != count_c: # (()("
        return False
    else:
        return True
    
       
