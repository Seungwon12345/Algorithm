# https://school.programmers.co.kr/learn/courses/30/lessons/120883

def solution(id_pw, db):
    answer = ''
    for i in range(len(db)):
        if db[i][0] == id_pw[0]: # ID 확인
            if db[i][1] == id_pw[1]: # PW 확인
                   answer = 'login'
            else:
                   answer = 'wrong pw'
            return answer
        
    answer = 'fail'
    return answer
