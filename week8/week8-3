# https://school.programmers.co.kr/learn/courses/30/lessons/42842

def solution(brown, yellow):
    area = brown + yellow
    answer = []
    for h in range(1, int(area**0.5)+1):  # 넓이가 9와 같이 제곱수 인 경우
        if area % h == 0:
            w = area // h
            
            if (w-2) * (h-2) == yellow:
                answer.append(w)
                answer.append(h)
            
    return answer
    
